{
  "scripts": {
    "build": "npx esbuild src/index.ts --bundle --platform=node --outfile=out.js",
    "check": "tsc src/* --noEmit --strict --target es2019 --moduleResolution node",
    "lint": "npx prettier -w src",
    "start": "npm run build && node out.js"
  },
  "dependencies": {
    "@types/node": "16.3.3",
    "esbuild": "0.12.15",
    "ts-md5": "^1.2.9",
    "typescript": "4.3.5"
  }
}
